    "keywords": ["laravel", "cron", "time ago", "from now", "time", "human"],
    "license": "MIT",
    "authors": [
        {
            "name": "Maarten Paauw",
            "email": "maartenpaauw@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Appsketch\\HumanCron\\": "src/"
        }
    },
    "require": {
        "mtdowling/cron-expression": "^1.0",
        "nesbot/carbon": "^1.20"
    },
    "require-dev": {
